{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "shipcloud.io",
    "description": "API of the shipping service provider shipcloud.io. Create shipping labels and track shipments for DHL, Deutsche Post, UPS, DPD, Hermes, GLS, MyDPD Business (iloxx), TNT, FedEx, Liefery and GO!",
    "contact": {
      "name": "André Cedik",
      "email": "developers@shipcloud.io",
      "url": "https://developers.shipcloud.io"
    }
  },
  "host": "api.shipcloud.io",
  "basePath": "/v1",
  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/addresses": {
      "get": {
        "description": "Get a list of addresses",
        "parameters": [
          {
            "name": "first_name",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by first name"
          },
          {
            "name": "last_name",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by last name"
          },
          {
            "name": "company",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by company name"
          },
          {
            "name": "care_of",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by care_of"
          },
          {
            "name": "street",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by street name"
          },
          {
            "name": "street_no",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by house number"
          },
          {
            "name": "zip_code",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by zip code"
          },
          {
            "name": "city",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by city"
          },
          {
            "name": "country",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by country code"
          },
          {
            "name": "phone",
            "in": "path",
            "type": "string",
            "description": "Filter addresses by phone number"
          },
          {
            "name": "page",
            "in": "path",
            "type": "integer",
            "description": "Number of page that should be returned"
          },
          {
            "name": "per_page",
            "in": "path",
            "type": "integer",
            "description": "Number of items that should be shown on a page",
            "default": 100
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/address"
              }
            }
          }
        }
      },
      "post": {
        "description": "An address for a shipments to or from address",
        "parameters": [
          {
            "name": "company",
            "in": "body",
            "type": "string",
            "description": "Company name"
          },
          {
            "name": "first_name",
            "in": "body",
            "type": "string"
          },
          {
            "name": "last_name",
            "in": "body",
            "type": "string",
            "required": true
          },
          {
            "name": "care_of",
            "in": "body",
            "type": "string"
          },
          {
            "name": "street",
            "in": "body",
            "type": "string",
            "required": true
          },
          {
            "name": "street_no",
            "in": "body",
            "type": "string",
            "required": true
          },
          {
            "name": "city",
            "in": "body",
            "type": "string",
            "required": true
          },
          {
            "name": "zip_code",
            "in": "body",
            "type": "string",
            "required": true
          },
          {
            "name": "country",
            "in": "body",
            "type": "string",
            "description": "Country as uppercase ISO 3166-1 alpha-2 code",
            "required": true
          },
          {
            "name": "phone",
            "in": "body",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "addresses response",
            "schema": {
              "$ref": "#/definitions/address"
            }
          }
        }
      }
    },
    "/addresses/{id}": {
      "get": {
        "description": "returns details of an address",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of an address",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "$ref": "#/definitions/address"
            }
          }
        }
      }
    },
    "/carriers": {
      "get": {
        "description": "Get all carriers available for your account",
        "responses": {
          "200": {
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/carrier"
              }
            },
            "examples": {
              "application/json": [
                {
                  "name": "dhl",
                  "display_name": "Deutsche Post DHL",
                  "services": [
                    "standard",
                    "returns",
                    "one_day",
                    "one_day_early"
                  ]
                },
                {
                  "name": "hermes",
                  "display_name": "Hermes Europe",
                  "services": [
                    "standard",
                    "returns"
                  ]
                },
                {
                  "name": "liefery",
                  "display_name": "Liefery",
                  "services": [
                    "same_day"
                  ]
                }
              ]
            }
          }
        }
      }
    },
    "/pickup_requests": {
      "post": {
        "description": "An address for a shipments to or from address",
        "parameters": [
          {
            "name": "carrier",
            "in": "body",
            "description": "acronym of the carrier a pickup should be requested for",
            "required": true,
            "type": "string",
            "enum": [
              "ups",
              "dpd",
              "hermes",
              "gls"
            ]
          },
          {
            "name": "pickup_time",
            "in": "body",
            "description": "defines a time window in which the carrier should pickup shipments",
            "required": true,
            "schema": {
              "$ref": "#/definitions/pickup_time"
            }
          },
          {
            "name": "pickup_date",
            "in": "body",
            "type": "string",
            "pattern": "^[0-9]{4}\/[0-9]{2}\/[0-9]{2}$",
            "description": "deprecated: please use **pickup_time** instead."
          },
          {
            "name": "shipments",
            "in": "body",
            "type": "array",
            "description": "array of shipment objects containing only their ids",
            "schema": {
              "$ref": "#/definitions/pickup_request_shipmment"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "pickup request response",
            "schema": {
              "$ref": "#/definitions/pickup_request"
            },
            "examples": {
              "application/json": {
                "id": "123456",
                "carrier": "dpd",
                "pickup_time": {
                  "earliest": "2015-09-15T09:00:00+02:00",
                  "latest": "2015-09-15T18:00:00+02:00"
                }
              }
            }
          }
        }
      }
    },
    "/shipment_quotes": {
      "post": {
        "description": "Creating a shipment quote",
        "parameters": [
          {
            "name": "carrier",
            "in": "body",
            "description": "acronym of the carrier you want to use",
            "required": true,
            "type": "string",
            "enum": [
              "dhl",
              "ups",
              "dpd",
              "hermes",
              "gls",
              "fedex",
              "liefery"
            ]
          },
          {
            "name": "service",
            "in": "body",
            "description": "The service that should be used for the shipment. standard: The standard (ground) service for each carrier. returns: Returns service. one_day: express delivery. one_day_early: express delivery until 10am.",
            "required": true,
            "type": "string",
            "enum": [
              "standard",
              "returns",
              "one_day",
              "one_day_early",
              "same_day"
            ],
            "default": "standard"
          },
          {
            "name": "from",
            "in": "body",
            "description": "The senders address. If missing, the default sender address (if defined in your shipcloud account) will be used",
            "required": false,
            "schema": {
              "$ref": "#/definitions/address"
            }
          },
          {
            "name": "to",
            "in": "body",
            "description": "the receivers address",
            "required": true,
            "schema": {
              "$ref": "#/definitions/address"
            }
          },
          {
            "name": "package",
            "in": "body",
            "description": "package dimensions",
            "required": true,
            "schema": {
              "$ref": "#/definitions/package"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "shipment response",
            "schema": {
              "$ref": "#/definitions/shipment_quote"
            },
            "examples": {
              "application/json": {
                "shipment_quote": {
                  "price": 42.12
                }
              }
            }
          }
        }
      }
    },
    "/shipments": {
      "get": {
        "description": "getting a list of shipments",
        "parameters": [
          {
            "name": "carrier",
            "in": "path",
            "type": "string",
            "description": "Filter shipments by carrier name"
          },
          {
            "name": "service",
            "in": "path",
            "type": "string",
            "description": "Filter shipments by service"
          },
          {
            "name": "reference_number",
            "in": "path",
            "type": "string",
            "description": "Filter shipments by reference number"
          },
          {
            "name": "carrier_tracking_no",
            "in": "path",
            "type": "string",
            "description": "Filter shipments by carrier tracking number"
          },
          {
            "name": "tracking_status",
            "in": "path",
            "type": "string",
            "description": "Filter shipments by our internal tracking status mappings (e.g. tracking_status=out_for_delivery)"
          },
          {
            "name": "page",
            "in": "path",
            "type": "integer",
            "description": "Number of page that should be returned"
          },
          {
            "name": "per_page",
            "in": "path",
            "type": "integer",
            "description": "Number of items that should be shown on a page",
            "default": 100
          }
        ],
        "responses": {
