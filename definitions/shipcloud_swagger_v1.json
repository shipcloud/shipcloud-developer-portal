      "post": {
        "description": "Creates a new shipment",
        "parameters": [
          {
            "name": "carrier",
            "in": "body",
            "description": "acronym of the carrier you want to use",
            "required": true,
            "type": "string",
            "enum": [
              "dhl",
              "ups",
              "dpd",
              "hermes",
              "gls",
              "fedex",
              "liefery"
            ]
          },
          {
            "name": "service",
            "in": "body",
            "description": "The service that should be used for the shipment. standard: The standard (ground) service for each carrier. returns: Returns service. one_day: express delivery. one_day_early: express delivery until 10am.",
            "required": true,
            "type": "string",
            "enum": [
              "standard",
              "returns",
              "one_day",
              "one_day_early",
              "same_day"
            ],
            "default": "standard"
          },
          {
            "name": "from",
            "in": "body",
            "description": "The senders address. If missing, the default sender address (if defined in your shipcloud account) will be used",
            "required": false,
            "schema": {
              "$ref": "#/definitions/address"
            }
          },
          {
            "name": "to",
            "in": "body",
            "description": "the receivers address",
            "required": true,
            "schema": {
              "$ref": "#/definitions/address"
            }
          },
          {
            "name": "package",
            "in": "body",
            "description": "package dimensions",
            "required": true,
            "schema": {
              "$ref": "#/definitions/package"
            }
          },
          {
            "name": "reference_number",
            "type": "string",
            "in": "body",
            "description": "a reference number (max. 30 characters) that you want this shipment to be identified with. You can use this afterwards to easier find the shipment in the shipcloud.io backoffice",
            "required": false
          },
          {
            "name": "description",
            "type": "string",
            "in": "body",
            "description": "text that describes the contents of the shipment. This parameter is mandatory if you're using UPS and the following conditions are true: from and to countries are not the same; from and/or to countries are not in the EU; from and to countries are in the EU and the shipments service is not standard",
            "required": false
          },
          {
            "name": "notification_email",
            "type": "string",
            "format": "email",
            "in": "body",
            "description": "email address that we should notify once there's an update for this shipment",
            "required": false
          },
          {
            "name": "additional_services",
            "in": "body",
            "description": "additional services",
            "required": false,
            "schema": {
              "$ref": "#/definitions/additional_services"
            }
          },
          {
            "name": "create_shipping_label",
            "type": "boolean",
            "in": "body",
            "description": "determines if a shipping label should be created at the carrier (**this means you will be charged**)",
            "required": false
          }
        ],
        "responses": {
          "200": {
            "description": "shipment response",
            "schema": {
              "$ref": "#/definitions/prepared_shipment"
            },
            "examples": {
              "application/json": {
                "id": "3a186c51d4281acbecf5ed38805b1db92a9d668b",
                "carrier_tracking_no": "84168117830018",
                "tracking_url": "http://track.shipcloud.io/3a186c51d4",
                "label_url": "http://api.shipcloud.io/shipping_label_3a186c51d4.pdf",
                "price": 3.4
              }
            }
          }
        }
      }
