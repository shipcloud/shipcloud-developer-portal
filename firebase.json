{
  "hosting": {
    "public": "_site",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/libraries",
        "destination": "/integrations",
        "type": 301
      },
      {
        "source": "/recipes",
        "destination": "/examples",
        "type": 301
      }
    ]
  }
}
